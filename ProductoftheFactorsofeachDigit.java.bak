import java.util.Scanner;
class  ProductoftheFactorsofeachDigit
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the value of Num");
		int num=sc.nextInt();
		int prod=0;
		for(int i=1;i<=num;i++)
		{
			if(num%i==0)
			{
				prod=prod*i;
			}
		}
		System.out.println("the product Factorial of the given Number is : "+prod);
	}
}
